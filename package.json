    "@mantine/modals": "^4.2.9",
    "@mantine/next": "^4.2.9",
    "@mantine/notifications": "^4.2.9",
    "@reduxjs/toolkit": "^1.7.2",
    "axios": "^0.25.0",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "next": "^12.1.6",
    "pretty-bytes": "^5.6.0",
