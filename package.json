{
  "name": "streamize-client",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier -w \"**/*.{js,ts,tsx,json}\"",
